<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/secondaryProgress">
        <shape

            android:shape="ring"
            android:thickness="13sp"
            android:useLevel="true">


            <gradient
                android:centerColor="#201f25"
                android:endColor="#201f25"
                android:startColor="#201f25"
                android:type="sweep" />
        </shape>
    </item>

    <item android:id="@android:id/progress">
        <rotate
            android:fromDegrees="270"
            android:pivotX="50%"
            android:pivotY="50%"
            android:toDegrees="270">

            <shape

                android:shape="ring"
                android:thickness="13sp"
                android:useLevel="true">


                <rotate
                    android:fromDegrees="0"
                    android:pivotX="50%"
                    android:pivotY="50%"
                    android:toDegrees="360" />

                <gradient
                    android:centerColor="#9c9da1"
                    android:endColor="#9c9da1"
                    android:startColor="#9c9da1"
                    android:type="sweep" />

            </shape>
        </rotate>
    </item>
</layer-list>